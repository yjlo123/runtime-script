/ Leetcode 1. Two Sum
/ by Siwei
/ Oct 2020

cal two_sum '[2,7,11,15]' 22
prt $ret

def two_sum
 prs input $0
 let target $1
 let result []
 let map {}
 let idx 0
 
 #loop
 pol $input num
 jeq $num $nil done
 get $map $num v
 ife $v $nil
  sub _key $target $num
  put $map $_key $idx
 els
  psh $result $v
  psh $result $idx
  ret $result
 fin
 add idx $idx 1
 jmp loop
 #done
 ret $result
end
