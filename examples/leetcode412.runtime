/ Leetcode 412. Fizz Buzz
/ by Siwei
/ Oct 2020

let n 17
let result []

cal fizz_buzz
prt $result

def fizz_buzz
 add n $n 1
 let i 1
 
 #loop
 jeq $i $n done
 mod m3 $i 3
 mod m5 $i 5
 add m3n5 $m3 $m5
 ife $m3n5 0
  psh $result 'FizzBuzz'
  jmp next
 fin
 ife $m3 0
  psh $result 'Fizz'
  jmp next
 fin
 ife $m5 0
  psh $result 'Buzz'
 els
  str i_str $i
  psh $result $i_str
 fin
 #next
 add i $i 1
 jmp loop
 #done
end
